<html>
    <head>
        <title></title>
    </head>
    <body>
        <main>
            <form action="/editstudent/<%= data.id %>/edit" method="POST">
                <label>Update Student</label>
                <input type="text" name="StudentName" value="<%= data.StudentName %>">
                <label>Update Parent Name</label>
                <input type="text" name="parentName" value="<%= data.Parent.name %>">
                <label>Update Parent Email</label>
                <input type="email" name="parentEmail" value="<%= data.Parent.email %>">
                <label>Update Parent Passcode</label>
                <input type="password" name="parentPassword" value="<%= data.Parent.password %>">
                <input type="submit" value="EDIT">
            </form>
        </main>
    </body>
</html>